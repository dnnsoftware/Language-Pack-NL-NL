  <data name="AdvancedSearchHint.Text" xml:space="preserve">
    <value>&lt;table class="dnnSearchResult-hint-tbl dnnTableDisplay"&gt;

                
				&lt;tr&gt;
                    &lt;th style="width: 150px"&gt;&lt;span&gt;Type&lt;/span&gt;
                    &lt;/th&gt;
                    &lt;th style="width: 250px"&gt;&lt;span&gt;Voorbeeld&lt;/span&gt;
                    &lt;/th&gt;
                    &lt;th&gt;&lt;span&gt;Aantekeningen&lt;/span&gt;
                    &lt;/th&gt;
				&lt;/tr&gt;
         

                &lt;tr&gt;
                    &lt;td&gt;Fuzzy&lt;/td&gt;
                    &lt;td&gt;kettle~&lt;/td&gt;
                    &lt;td&gt;Bevat termen die in de buurt komen van het woord&lt;i&gt;ve&lt;/i&gt;, als in &lt;i&gt;vee&lt;/i&gt;&lt;/td&gt;
                &lt;/tr&gt;

                &lt;tr&gt;
                    &lt;td&gt;Wild&lt;/td&gt;
                    &lt;td&gt;cat*&lt;/td&gt;
                    &lt;td&gt;Bevat termen die beginnen met &lt;i&gt;stof&lt;/i&gt;, als in &lt;i&gt;stofzuiger&lt;/i&gt; en de exacte term &lt;i&gt;stof&lt;/i&gt; zelf&lt;/td&gt;
                &lt;/tr&gt;

                &lt;tr&gt;
                    &lt;td&gt;Enkele onttrekking&lt;/td&gt;
                    &lt;td&gt;orange&lt;/td&gt;
                    &lt;td&gt;bevat de term&lt;i&gt;orange&lt;/i&gt;&lt;/td&gt;
                &lt;/tr&gt;

                &lt;tr&gt;
                    &lt;td&gt;exacte zin&lt;/td&gt;
                    &lt;td&gt;"dnn is geweldig "&lt;/td&gt;
                    &lt;td&gt;Bevat de exacte zin &lt;i&gt;dnn is geweldig&lt;/i&gt;&lt;/td&gt;
                &lt;/tr&gt;

                &lt;tr&gt;
                    &lt;td&gt;of&lt;/td&gt;
                    &lt;td&gt;orange fiets&lt;/td&gt;
                    &lt;td&gt;bevat de term &lt;i&gt;orange&lt;/i&gt; of &lt;i&gt;fiets&lt;/i&gt;, of beide. &lt;b&gt;&lt;i&gt;Of&lt;/i&gt;,moet in hoofdletters&lt;/b&gt;&lt;/td&gt;
                &lt;/tr&gt;

                &lt;tr&gt;
                    &lt;td&gt;&lt;/td&gt;
                    &lt;td&gt;orange OF Fiets&lt;/td&gt;
                    &lt;td&gt;&lt;/td&gt;
                &lt;/tr&gt;

                &lt;tr&gt;
                    &lt;td&gt;en&lt;/td&gt;
                    &lt;td&gt;+orange +fiets&lt;/td&gt;
                    &lt;td&gt;bevat beide &lt;i&gt;orange&lt;/i&gt; en &lt;i&gt;fiets&lt;/i&gt;&lt;/td&gt;
                &lt;/tr&gt;

                &lt;tr&gt;
                    &lt;td&gt;&lt;/td&gt;
                    &lt;td&gt;orange en fiets&lt;/td&gt;
                    &lt;td&gt;&lt;/td&gt;
                &lt;/tr&gt;

                &lt;tr&gt;
                    &lt;td&gt;combinatie&lt;/td&gt;
                    &lt;td&gt;(behendig of extreem) en methodology&lt;/td&gt;
                    &lt;td&gt;bevat &lt;i&gt;methodology&lt;/i&gt; en moet ook bevatten &lt;i&gt;behendig&lt;/i&gt; en/of &lt;i&gt;extreem&lt;/i&gt;&lt;/td&gt;
                &lt;/tr&gt;
            &lt;/table&gt;</value>
